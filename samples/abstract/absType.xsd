<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified" xmlns:ns1="http://test.com/source/" targetNamespace="http://test.com/source/">
    <xsd:complexType name="PublicationType" abstract="true">
        <xsd:sequence>
            <xsd:element name="Title" type="xsd:string"/>
            <xsd:element name="Author" type="xsd:string" 
                         minOccurs="0" maxOccurs="unbounded"/>
            <xsd:element name="Date" type="xsd:string"/>
        </xsd:sequence>
    </xsd:complexType>
	
	<xsd:element name="Catalogue">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="Publication" type="ns1:PublicationType" 
                             maxOccurs="unbounded"/>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
	
  <xsd:complexType name="BookType">
        <xsd:complexContent>
            <xsd:extension base="ns1:PublicationType">
                <xsd:sequence>
                    <xsd:element name="ISBN" type="xsd:string"/>
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>

    <xsd:complexType name="MagazineType">
        <xsd:complexContent>
            <xsd:extension base="ns1:PublicationType"/>
        </xsd:complexContent>
    </xsd:complexType>	
</xsd:schema>